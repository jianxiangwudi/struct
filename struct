#include <stdio.h>
typedef struct stu
{
	char name[20];
	char sex[20];
	int a;
	char tele[20];
}stu;
//void print(stu ab)
//{
//	printf("个人基本信息");
//	printf("姓名：%s\n", ab.name);
//	printf("性别：%s\n", ab.sex);
//	printf("年龄：%d\n", ab.a);
//	printf("电话号码：%s\n", ab.tele);
//}
void print(stu* ab)
{
	printf("姓名：%p\n", ab->name);
	printf("性别：%p\n", ab->sex);
	printf("年龄：%d\n", ab->a);
	printf("电话号码：%p\n", ab->tele);
}
int main()
{
	stu a = { "张三","男",20,"13245675246" };
	print(&a);
	return 0;
}
